/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // de morgans laws:
    //Not(x) AND Not(y) = NOT(x OR y)
    Not(in=a , out= nota);
    Not(in= b, out= notb);
    And(a= nota, b=notb , out= notanotb);
    Not(in= notanotb, out=out );
}